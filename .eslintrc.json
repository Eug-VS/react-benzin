{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-typescript",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "ignorePatterns": "dist/",
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": ["error", "single"]
  }
}
